<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
	
    <meta name="description" content="Billigast på Prisjakt av Magnus Kläppe.">
    <meta name="author" content="Magnus Kläppe 2016">
    <!--<meta http-equiv="refresh" content="60">-->

    <meta name="keywords" content="billigast,prisjakt,magnus kläppe,kläppe">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Prisjakt - De bästa priserna av Magnus Kläppe</title>
    <link href="/Content/bootstrap.css" type="text/css" rel="stylesheet" />
    <link href="/Content/Site.css" type="text/css" rel="stylesheet" />
	
	<script type="text/javascript" src="/Scripts/modernizr-2.6.2.js"></script>

</head>
<body>

    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Prisjakt", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>

        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Billigast på Prisjakt av Magnus Kläppe</p>
        </footer>
    </div>
	<script type="text/javascript" src="/Scripts/jquery-1.10.2.min.js"></script>
<script type="text/javascript" src="/Scripts/bootstrap.min.js"></script>


<script type="text/javascript" src="/Scripts/Prisjakt.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
